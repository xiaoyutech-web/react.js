{
  "name": "react-webapp",
  "version": "1.0.0",
  "description": "react webapp",
  "main": "index.js",
  "scripts": {
    "test": "better-npm-run test",
    "clean": "rimraf index.html dist",
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "eslintfix": "eslint src/**/*.js --fix",
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.scss"
  },
  "betterScripts": {
    "test": {
      "command": "karma start ./test/unit/karma.conf.js"
    },
    "start": {
      "command": "webpack-dev-server --config build --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean && webpack -p --config build --define process.env.NODE_ENV='\"production\"' --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "react"
  ],
  "author": "zhaojintian",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "better-npm-run": "^0.0.15",
    "bundle-loader": "^0.5.5",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.7",
    "jsx-loader": "^0.13.2",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.9.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-font-magician": "^1.6.1",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.8",
